            <string>/// Enemy State Machine

switch (state) {
    case (EnemyState.Idle):
        // Look out for player
        spd = lerp(spd, 0, 0.1 * f_time_scale());
        var player = instance_nearest(x, y, o_player);
        if (player != noone &amp;&amp; distance_to_object(player) &lt; 200) {
            target = player;
            state = EnemyState.Walk;
        }
        break;

    case (EnemyState.Walk):
        // Move towards player
        spd = lerp(spd, spd_max, 0.05 * f_time_scale());
        
        if (distance_to_object(target) &lt; 50) {
            state = EnemyState.Attack;
        } else if (distance_to_object(target) &gt; 200) {
            state = EnemyState.Idle;
        }
        break;

    case (EnemyState.Attack):
        // Attempt to hurt player

        spd = lerp(spd, 0, 0.1 * f_time_scale());
        if (count &lt; count_min) {
            count += f_time_scale();
            if (
                count mod 10 &lt; 0.1 &amp;&amp;
                target != noone &amp;&amp;
                distance_to_object(target) &lt; 30 &amp;&amp;
                variable_instance_exists(target, 'hp')
            ) {
                target.hp -= 5;
            }
        } else {
            count = 0;
            state = EnemyState.Idle;
        }
        break;

    case (EnemyState.Dead):
        spd = 0;
        target = noone;
        alpha -= 0.001 * f_time_scale();
        image_alpha = alpha;
        if (alpha &lt;= 0) {
            instance_destroy();
        }
        break;

    default:
        state = EnemyState.Idle;
        break;
}

if (target != noone) {
    move_towards_point(target.x, target.y, spd * f_time_scale());
} else {
    speed = 0;
}

if (hp &lt;= 0) {
    state = EnemyState.Dead;
}

</string>
