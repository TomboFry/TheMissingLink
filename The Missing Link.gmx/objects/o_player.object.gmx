          <argument>
            <kind>1</kind>
            <string>/// Fire Weapon

// Update current weapon
current_weapon = inventory.inventory[# inventory_slot_selected, 0];

if (f_key(Controls.WeaponFire)) {
    if (current_weapon != noone &amp;&amp; current_weapon.fire_object != noone &amp;&amp; current_weapon.wait_count == 0) {
        if (current_weapon.ammo &gt; 0) {
            if (
                (current_weapon.fire_rate == 0 &amp;&amp; fire_held == 0) ||
                (current_weapon.fire_rate &gt; 0 &amp;&amp; fire_held mod current_weapon.fire_rate == 0)
            ) {
                var fire = instance_create(x, y - 10, current_weapon.fire_object);
                fire.parent = self;
                fire.damage = current_weapon.damage;
                with (fire) event_user(0);
        
                current_weapon.ammo--;
                current_weapon.wait_count = current_weapon.fire_wait;
            }
        } else {
            // Play empty clip sound
        }
    }

    fire_held++;
}

if (f_key_up(Controls.WeaponFire)) fire_held = 0;

</string>
